#MyProfileContainer
  width: 85%
  height: 100%
  display: flex
  flex-direction: column
  padding: 1em 4em


#MyProfilePresentation
  display: flex
  flex-direction: column
  justify-content: center
  align-items: center
  width:400px
  margin: 1em auto 1em
  padding:1em
  background-color: #EC9C34
  border-radius: 10px

#MyProfilePhoto
  width: 100px
  height: 100px
  border-radius: 100%
  margin-bottom: 1em

#MyProfileDescription
  opacity: .6
  font-size: .7em


#settingsForMyProfileTrigger
  position: absolute
  top: 120px
  left:500px


#MyProfileBasicsInformation
  display: flex
  flex-direction: row
  flex-wrap: wrap
  justify-content: space-around
  margin: .5em auto
  padding: 1em 0
  border-bottom: thick double #EC9C34
  border-top: thick double #EC9C34

.BasicsInformation
  width:40%
  padding: .5em 2em
  margin-bottom: 1em
  background-color: black
  color: white
  opacity: .6
  font-size: .8em
  border-radius: 10px

#AddOrDeleteFriendFromMyProfile
  display: flex
  flex-direction: row
  flex-wrap: nowrap
  justify-content: space-around
  margin: .5em auto

.FriendsFromMyProfile
  width:40%
  padding: .5em 2em
  margin-bottom: 1em
  border-radius: 10px
