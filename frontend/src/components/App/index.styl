@require "frontend/styles/charte";

body
  font-size: 1.7em;
  font-family: 'Montserrat', sans-serif;
  background-color: $second-color;
  box-sizing: border-box;


#app > div:not(#homeVisiter)
	display: flex;
	flex-direction: row;

.resize-pic
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: 50% 0%;

.right-side
  display: grid;
  grid-template-rows: 55px 1fr 60px;
  width: 100%;
